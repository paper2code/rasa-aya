{
    "AWSEBDockerrunVersion": 2,
    "volumes": [
        {
          "host": {
                "sourcePath": "/var/app/current/src"
            },
            "name": "src"
        }
      ],
    "containerDefinitions": [
        {
            "name": "flask",
            "image": "s0288/flask",
            "hostname": "flask",
            "essential": false,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000
                }
            ],
            "memory": 900
        }
    ]
}