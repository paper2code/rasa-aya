{
    "AWSEBDockerrunVersion": 2,
    "volumes": [
        {
          "host": {
                "sourcePath": "/var/app/current/rasa"
            },
            "name": "Rasa"
        },
        {
            "host": {
                "sourcePath": "/var/app/current/rasa/actions"
            },
            "name": "Actions"
        }
      ],
    "containerDefinitions": [
        {
            "name": "telegram",
            "image": "s0288/telegram",
            "hostname": "telegram",
            "essential": true,
            "links": ["rasa"],
            "memory": 128
        },
        {
            "name": "flask",
            "image": "s0288/flask",
            "hostname": "flask",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000
                }
            ],
            "memory": 128
        },
        {
            "name": "rasa",
            "image": "rasa/rasa:1.10.3-full",
            "hostname": "rasa",
            "essential": true,
            "memory": 900,
            "memoryReservation": 512,
            "command": ["run"],
            "mountPoints": [
                {
                  "containerPath": "/app",
                  "sourceVolume": "Rasa"
                }
              ],
            "portMappings": [
                {
                    "containerPort": 5005,
                    "hostPort": 5005
                }
            ],              
            "links": ["flask", "actions"]
        },
        {
            "name": "actions",
            "image": "rasa/rasa-sdk:1.10.1",
            "hostname": "actions",
            "essential": true,
            "memory": 128,
            "memoryReservation": 90,
            "command": ["start", "--actions", "actions"],
            "mountPoints": [
                {
                  "containerPath": "/app/rasa/actions",
                  "sourceVolume": "Actions"
                }
              ],
            "portMappings": [
                {
                    "hostPort": 5055,
                    "containerPort": 5055
                }
            ]
        }
    ]
}