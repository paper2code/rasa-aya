{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "telegram",
            "image": "s0288/telegram",
            "hostname": "telegram",
            "essential": true
        },        {
            "name": "flask",
            "image": "s0288/flask",
            "hostname": "flask",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000
                }
            ],
            "links": ["rasa"]
        },
        {
            "name": "rasa",
            "image": "s0288/rasa",
            "hostname": "rasa",
            "essential": false,
            "portMappings": [
                {
                    "hostPort": 5005,
                    "containerPort": 5005
                }
            ],
            "links": ["flask", "actions"]
        },
        {
            "name": "actions",
            "image": "s0288/actions",
            "hostname": "actions",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5055,
                    "containerPort": 5055
                }
            ],
            "links": ["rasa"]
        }
    ]
}