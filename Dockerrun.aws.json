{
    "AWSEBDockerrunVersion": 2,
    "volumes": [
        {
          "name": "rasa-volume",
          "host": {
            "sourcePath": "./app/rasa"
          }
        }  
      ],
    "containerDefinitions": [
        {
            "name": "telegram",
            "image": "s0288/telegram:latest",
            "hostname": "telegram",
            "essential": true,
            "links": ["rasa-app"],
            "memory": 128
        },
        {
            "name": "flask",
            "image": "s0288/flask:latest",
            "hostname": "flask",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000
                }
            ],
            "memory": 128
        },
        {
            "name": "rasa",
            "image": "s0288/rasa:latest",
            "hostname": "rasa-app",
            "essential": false,
            "portMappings": [
                {
                    "hostPort": 5005,
                    "containerPort": 5005
                }
            ],
            "mountPoints": [
                {
                  "sourceVolume": "rasa-volume",
                  "containerPath": "./app"
                }
              ],
            "links": ["flask", "actions"],
            "memoryReservation": 512
        },
        {
            "name": "actions",
            "image": "s0288/actions:latest",
            "hostname": "actions",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5055,
                    "containerPort": 5055
                }
            ],
            "memory": 128
        }
    ]
}